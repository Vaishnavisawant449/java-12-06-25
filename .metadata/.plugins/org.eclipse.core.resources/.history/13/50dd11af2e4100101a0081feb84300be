package tyco;

public class constructor_demo {
	int num1,num2;
 constructor_demo() {
	System.out.println("default constructor");

}
	constructor_demo(int a ,int b){
		num1=a;
		num2=b;
		
	System.out.printl(a+" "+b);
}constructor_demo(constructor_demo a){
  
		
	public static void main(String[] args) {
		constructor_demo c=new constructor_demo(71);
		constructor_demo c2=new constructor_demo(10,20);
		constructor_demo c3=new constructor_demo(c1);
		
	}

}
