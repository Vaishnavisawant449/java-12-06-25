package tyco;

class Employee{
	void msg(int salary) {
		System.out.println("Parent Class Data"+salary);
	}
	
}
public class HR extends Employee {
	void msg(int salary) {
		super.msg(20000);
		System.out.println("Child Class Data");

	}
	
	public static void main(String[] args) {
		HR mm=new HR();
		mm.msg(10000);
		

	}

}
